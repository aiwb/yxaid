    "jquery": "^3.2.1",
    "postcss": "^6.0.6",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.2",
    "redux": "^3.4.0",
